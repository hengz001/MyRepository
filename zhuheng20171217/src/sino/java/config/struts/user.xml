    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<include file="struts-default.xml" />
	<package name="user" namespace="/" extends="struts-default">
		<action name="user_*" class="sino.java.action.user.UserAction" method="{1}">
			<result name="show">/WEB/page/user/index2.jsp</result>
			<result name="success" type="chain">user_findByPage</result>
			<result name="findByPage">/WEB/page/user/findByPage.jsp</result>
		</action>
	</package>
</struts>
