<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<include file="struts-default.xml" />
	<package name="emp" namespace="/" extends="struts-default">
		<action name="emp_*" class="sino.java.action.emp.EmpAction" method="{1}">
			<result name="index">/WEB/page/emp/employeeManage.jsp</result>
			<result name="addEmp">/WEB/page/common/close.html</result>
			<result name="deleteEmp">/WEB/page/common/close.html</result>
			<result name="showEmp">/WEB/page/emp/editEmp.jsp</result>
		</action>
	</package>
</struts>

