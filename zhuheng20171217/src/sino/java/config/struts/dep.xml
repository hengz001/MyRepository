<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<include file="struts-default.xml" />
	<package name="dep" namespace="/" extends="struts-default">
		<action name="dep_*" class="sino.java.action.dep.DepAction" method="{1}">
			<result name="show">/WEB/page/dep/index3.jsp</result>
			<result name="index">/WEB/page/dep/departmentManage.jsp</result>
			<result name="showAddDep">/WEB/page/dep/addDepartment.jsp</result>
			<result name="addDep">/WEB/page/common/close.html</result>
			<result name="deleteDep">/WEB/page/common/close.html</result>
		</action>
	</package>
</struts>
