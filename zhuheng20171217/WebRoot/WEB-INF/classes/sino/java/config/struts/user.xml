<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<include file="struts-default.xml" />
	<package name="user" namespace="/" extends="struts-default">
		<action name="user_*" class="sino.java.action.user.UserAction" method="{1}">
			<result name="showAddUser">/WEB/page/user/addaccount.jsp</result>
			<result name="getEmps">/WEB/page/user/selectemp.jsp</result>
			<result name="addUser">/WEB/page/common/close.html</result>
			<result name="deleteUser">/WEB/page/common/close.html</result>
			<result name="login">/WEB/page/main/main.jsp</result>
			<result name="unlogin">/index.jsp</result>
		</action>
	</package>
</struts>
