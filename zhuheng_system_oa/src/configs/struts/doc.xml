<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<include file="struts-default.xml" />
	<package name="doc" namespace="/" extends="struts-default">
		<action name="doc_*" class="sino.java.action.doc.DocAction" method="{1}">
			<result name="addDoc">/WEB/page/doc/addDoc01.jsp</result>
			<result name="addDoc02">/WEB/page/doc/addDoc02.jsp</result>
			<result name="addDoc03">/WEB/page/common/close.html</result>
			<result name="myDoc">/WEB/page/doc/myDoc.jsp</result>
			<result name="openSubmitDoc">/WEB/page/doc/openSubmitDoc.jsp</result>
		</action>
	</package>
</struts>

	

	