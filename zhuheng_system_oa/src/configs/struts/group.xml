<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<include file="struts-default.xml" />
	<package name="group" namespace="/" extends="struts-default">
		<action name="group_*" class="sino.java.action.group.GroupAction" method="{1}">
			<result name="index">/WEB/page/group/account.jsp</result>
			<result name="showAddGrp">/WEB/page/group/addgroup.jsp</result>
			<result name="addGrp">/WEB/page/common/close.html</result>
			<result name="deleteGrp">/WEB/page/common/close.html</result>
			<result name="openUpdateGroup">/WEB/page/group/editgroup.jsp</result>
			<result name="updateGroup">/WEB/page/common/close.html</result>
		</action>
	</package>
</struts>

	

	